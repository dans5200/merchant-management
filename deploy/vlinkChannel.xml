<channel-adaptor name='vlinkChannel' class="org.jpos.q2.iso.ChannelAdaptor" logger="Q2">
    <channel class="org.jpos.iso.channel.ASCIIChannel" logger="Q2" packager="org.jpos.iso.packager.GenericPackager">
        <property name="host" value="localhost" />
        <property name="port" value="8010" />
        <property name="packager-config" value="packager/customePackager.xml" />
        <property name="timeout" value="300000" />
        <property name="keep-alive" value="true" />
    </channel>
    <in>jpos-receive</in>
    <out>jpos-send</out>
    <reconnect-delay>10000</reconnect-delay>
</channel-adaptor>